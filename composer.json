{
  "name": "piedweb/curl",
  "type": "library",
  "description": "curl OOP Wrapper",
  "keywords": [
    "Pied Web",
    "Curl"
  ],
  "homepage": "https://dev.piedweb.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Robin D. (ak Pied Web)",
      "email": "contact@robin-d.fr",
      "homepage": "https://piedweb.com"
    }
  ],
  "require": {
    "php": "^7.1|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
